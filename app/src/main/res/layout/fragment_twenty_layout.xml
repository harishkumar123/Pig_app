<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >
    <LinearLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:orientation="vertical">

        <TextView
            android:layout_marginTop="55dp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:gravity="center"
            android:id="@+id/text1"/>
<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.akmu.pig_app.twenty_fragment">
    <LinearLayout
        android:id="@+id/layout11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <TextView
        android:id="@+id/TextWAWHdng"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="20dp"
        android:paddingBottom="20dp"
        android:textSize="25sp"
        android:gravity="center"
        android:textColor="#000000"
        android:text="शूकर पालन प्रोजेक्ट" />
    </LinearLayout>
    <Button
        android:id="@+id/parikalpana_text"
        android:layout_width="match_parent"
        android:layout_height="55sp"
        android:layout_marginBottom="10dp"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:background="@color/orange"
        android:layout_below="@+id/layout1"
        android:text="परिकल्पनायें"
        />
   <LinearLayout
       android:id="@+id/layout1"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_below="@+id/layout11"
       android:orientation="vertical">

       <View
           android:layout_width="match_parent"
           android:layout_height="10dp"/>
       <TextView
           android:layout_height="wrap_content"
           android:layout_width="match_parent"
           android:id="@+id/text21"/>

       <View
           android:layout_width="match_parent"
           android:layout_height="10dp"/>
       <TextView
           android:layout_height="wrap_content"
           android:layout_width="match_parent"
           android:id="@+id/text22"/>

       <View
           android:layout_width="match_parent"
           android:layout_height="10dp"/>
       <TextView
           android:layout_height="wrap_content"
           android:layout_width="match_parent"
           android:id="@+id/text23"/>

       <View
           android:layout_width="match_parent"
           android:layout_height="10dp"/>
       <TextView
           android:layout_height="wrap_content"
           android:layout_width="match_parent"
           android:id="@+id/text24"/>

       <View
           android:layout_width="match_parent"
           android:layout_height="10dp"/>
       <TextView
           android:layout_height="wrap_content"
           android:layout_width="match_parent"
           android:id="@+id/text25"/>

       <View
           android:layout_width="match_parent"
           android:layout_height="10dp"/>
       <TextView
           android:layout_height="wrap_content"
           android:layout_width="match_parent"
           android:id="@+id/text26"/>

       <View
           android:layout_width="match_parent"
           android:layout_height="10dp"/>
       <TextView
           android:layout_height="wrap_content"
           android:layout_width="match_parent"
           android:id="@+id/text27"/>

       <View
           android:layout_width="match_parent"
           android:layout_height="10dp"/>
   </LinearLayout>
    <Button
        android:id="@+id/taknikimanak_text"
        android:layout_width="match_parent"
        android:layout_height="55sp"
        android:layout_marginBottom="10dp"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:background="@color/orange"
        android:layout_below="@id/layout2"
        android:text="तकनीकी मानक"
        />
    <LinearLayout
        android:id="@+id/layout2"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_below="@+id/parikalpana_text"
        android:orientation="vertical"
        tools:ignore="WebViewLayout">

        <WebView
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:id="@+id/webView1"
           />
    </LinearLayout>
    <Button
        android:id="@+id/aarthikmanak_text"
        android:layout_width="match_parent"
        android:layout_height="55sp"
        android:layout_marginBottom="10dp"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:background="@color/orange"
        android:layout_below="@+id/layout3"
        android:text="आर्थिक मानक"
        />
    <LinearLayout
        android:id="@+id/layout3"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_below="@+id/taknikimanak_text"
        android:orientation="vertical">

        <WebView
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:id="@+id/webView2"
            />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <TextView
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:layout_below="@+id/aarthikmanak_text"
        android:id="@+id/text28"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <Button
        android:id="@+id/project_1_text"
        android:layout_width="match_parent"
        android:layout_height="55sp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:background="@color/orange"
        android:layout_below="@+id/text28"
        android:text="प्रोजेक्ट -I (10 प्रजनक मादा + 2 प्रजनक नर)"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <Button
        android:id="@+id/project_2_text"
        android:layout_width="match_parent"
        android:layout_height="55sp"
        android:layout_marginBottom="10dp"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:background="@color/orange"
        android:layout_below="@+id/project_1_text"
        android:text="प्रोजेक्ट -II (20 प्रजनक मादा + 2 प्रजनक नर)"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <Button
        android:id="@+id/project_3_text"
        android:layout_width="match_parent"
        android:layout_height="55sp"
        android:layout_marginBottom="10dp"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:background="@color/orange"
        android:layout_below="@+id/project_2_text"
        android:text="प्रोजेक्ट -III (50 प्रजनक मादा + 4 प्रजनक नर)"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <Button
        android:id="@+id/project_4_text"
        android:layout_width="match_parent"
        android:layout_height="55sp"
        android:layout_marginBottom="10dp"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:background="@color/orange"
        android:layout_below="@+id/project_3_text"
        android:text="प्रोजेक्ट -IV (100 प्रजनक मादा + 5 प्रजनक नर)"
        />


</RelativeLayout>
    </LinearLayout>
</ScrollView>
